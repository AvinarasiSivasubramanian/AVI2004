tart=int(input ("Enter the starting Value: "))  
end=int(input ("Enter the ending Value: "))  
print ("The Prime Numbers in the range are: ")  
for number in range (start,end+1):  
    if number > 1:  
        for i in range (2, number):  
            if (number % i) == 0:  
                break  
        else:  
            print (number)
    else:
        print("Invalid entry")
